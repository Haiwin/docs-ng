    <head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title>{{{site-title}}}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width">
	<!--<link rel="stylesheet" href="http://yandex.st/highlightjs/7.3/styles/default.min.css">-->
	<link rel="stylesheet" href="http://yandex.st/highlightjs/7.3/styles/tomorrow.min.css">
	<script src="http://yandex.st/highlightjs/7.3/highlight.min.js"></script>
	<!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
	{{#stylesheet_bundle}}/css/all.css{{/stylesheet_bundle}}
    </head>

    <body data-spy="scroll" data-target="#sidenav">
	<div class="container-fluid">
	    <div class="masthead">
		<div id="cse-search" class="navbar-search pull-right">
		    <gcse:searchbox></gcse:searchbox>
		</div>
		<ul class="nav nav-pills pull-right">
		    {{#navbar}}
		    <li class="{{#if subnav}}dropdown {{/if}}{{active_class .}}">
			<a {{#if subnav}}class="dropdown-toggle" data-toggle="dropdown"{{/if}} href="{{{href}}}">
			    {{label}}{{#if subnav}}<b class="caret"></b>{{/if}}</a>
			{{#if subnav}}
			<ul class="dropdown-menu" role= "menu">
			    {{#subnav}}
			    <li><a tabindex="-1" href="{{{href}}}">{{label}}</a>
			    </li>
			    {{/subnav}}
			</ul>
			{{/if}}
		    </li>
		    {{/navbar}}
		    <li>
		        <a href="http://www.couchbase.com/docs/index-ja.html"><img src="/images/ja.png"></a>
		    </li>
		    	    <li>
        		        <a href="http://www.couchbase.com/kr"><img src="/images/ko.png"></a>
        		    </li>
		</ul>
		<h3>{{site-title}}</h3>
	    </div>
	    <div id="cse-results">
		<gcse:searchresults></gcse:searchresults>
	    </div>
	    <div class="row-fluid">
		<div class="span3 hidden-phone">
		    <ul class="span3 nav nav-list" id="sidenav">
		    </ul>
		</div>
