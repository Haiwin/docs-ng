    <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
      <title><%= @item[:title] %></title>
      <meta name="description" content="">
      <meta name="viewport" content="width=device-width">
      <link type="image/x-icon" href="/assets/img/nosql_favicon.ico" rel="shortcut icon">
      <%= css_link("/assets/css/main") %>
      
      <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-7763794-1']);
      _gaq.push(['_setDomainName', '.couchbase.com']);
      _gaq.push(['_setAllowLinker', true]);
      _gaq.push(['_setAllowHash', false]);
      _gaq.push(['_trackPageview']);
      (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
      </script>
      
    </head>

    <body data-spy="scroll" data-target="#sidenav">

            <div class="container-fluid">
         
                <div class="masthead">
                    <!-- if masthead height changes, make sure you also change jumptarget height in main.less-->
                 
                    <div id="cse-search" class="navbar-search pull-right">
                         
                        <gcse:searchbox></gcse:searchbox>
                      
                        <br />
                       
                          <div class="version pull-right"><%= render 'version_widget' %></div>
                     
                        <!--<ul class = "nav nav-pills pull-right">
                         
                            <li><a href="#">hello</a></li>
                              <li><a href="#">world</a></li>
                            </ul> -->


                    </div>
         
                    <ul class="nav nav-pills pull-right">
                        <li>
                            <a href="/index.html"><%= image_tag("/assets/img/home_icon") %></a>
                        </li>
                        <%= render 'navbar_widget' %>
                        <li>
                            <a href="http://www.couchbase.com/docs/index-ja.html"><%= image_tag("/assets/img/ja") %></a>
                        </li>
                        <li>
                            <a href="http://www.couchbase.com/kr"><%= image_tag("/assets/img/ko") %></a>
                        </li>
                    </ul>
          
                    <h3 class="cb_logo"><a href="http://www.couchbase.com"><%= image_tag("/assets/img/couchbase_logo") %></a></h3>
                </div>
     
             
            <div id="cse-results">
                <gcse:searchresults></gcse:searchresults>
            </div>
    
          
        <div class="row-fluid">
            <% unless @item[:no_toc] %>
                <%= render 'toc_widget' %>
                <% end %>
            <div class="span9 mdcontent <%= @item[:css_class] %>">
           
                <%= yield %>
            </div>
        </div>
      
      </div>
    
    </div>
   

     
    <%= js_link("/assets/js/main") %>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script>
      (function() {
        var cx = '006614600140080434676:vzbmpgopyd4';
        var gcse = document.createElement('script'); gcse.type = 'text/javascript';
        gcse.async = true;
        gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
        '//www.google.com/cse/cse.js?cx=' + cx;
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(gcse, s);
       })();
        $("a[data-toggle=popover]").popover().click(function(e) {
            e.preventDefault()
