				<colspec colname="col1" colwidth="1*" class="- topic/colspec " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Concepts/concept-dataStorage.dita" xtrc="colspec:1;23:44"></colspec>
				<colspec colname="col2" colwidth="5.06*" class="- topic/colspec " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Concepts/concept-dataStorage.dita" xtrc="colspec:2;24:47"></colspec>
