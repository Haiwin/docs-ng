			<strow class="- topic/strow " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cbcollect-info-tool.dita" xtrc="strow:2;39:11">
				<stentry class="- topic/stentry " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cbcollect-info-tool.dita" xtrc="stentry:5;40:14">
					--upload-host=hostname
					<p class="- topic/p " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cbcollect-info-tool.dita" xtrc="p:4;42:9">-n hostname</p>
				</stentry>
