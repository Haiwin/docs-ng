    --settings \\	
    --xdcr-replicator=f4eb540d74c43fd3ac6d4b7910c8c92f/default/default \\	
    --max-concurrent-reps=32 \\	
    --checkpoint-interval=1800 \\	
    --worker-batch-size=500 \\	
    --doc-batch-size=2048 \\	
