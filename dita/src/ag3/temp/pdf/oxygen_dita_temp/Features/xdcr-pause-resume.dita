      <section class="- topic/section " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="section:2;97:16"><title class="- topic/title " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="title:4;97:23">Syntax</title>
        <codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="codeblock:1;98:20">
couchbase-cli xdcr-replicate -c HOST:PORT
    --pause	
    --xdcr-replicator=REPLICATOR	
    -u ADMIN -p PASSWORD         
 
couchbase-cli xdcr-replicate -c HOST:PORT
  --resume	
  --xdcr-replicator=REPLICATOR	
  -u ADMIN -p PASSWORD
        </codeblock>
        
      </section>
      

    <example class="- topic/example " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="example:1;113:14"><title class="- topic/title " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="title:5;113:21">Pausing a running replication stream</title>
