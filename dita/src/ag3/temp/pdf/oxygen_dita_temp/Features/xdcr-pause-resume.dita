      <example class="- topic/example " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="example:3;133:16"><title class="- topic/title " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="title:7;133:23">Updating settings for a replication stream</title>
      <codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/xdcr-pause-resume.dita" xtrc="codeblock:4;134:18">
