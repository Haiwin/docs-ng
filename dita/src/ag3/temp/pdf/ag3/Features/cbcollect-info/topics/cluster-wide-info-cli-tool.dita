				<kwd class="+ topic/keyword pr-d/kwd " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="kwd:4;37:10">collect-logs-stop</kwd>
				<option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:9;38:13">-c</option>
				<var class="+ topic/ph pr-d/var " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="var:8;39:10">host</var>:8091 <option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:10;39:34">-u</option>
				<var class="+ topic/ph pr-d/var " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="var:9;40:10">username</var>
				<option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:11;41:13">-p</option>
				<var class="+ topic/ph pr-d/var " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="var:10;42:10">password</var>
			</synph>
		</section>

