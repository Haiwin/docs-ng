				<delim class="+ topic/ph pr-d/delim " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="delim:2;24:12">|</delim>
				<option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:5;25:13">--all-nodes</option>
				<delim class="+ topic/ph pr-d/delim " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="delim:3;26:12">}</delim> [--upload <option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:6;26:40">--upload-host</option><delim class="+ topic/ph pr-d/delim " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="delim:4;26:69">=</delim><var class="+ topic/ph pr-d/var " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="var:5;26:83">host</var>
				<option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:7;27:13">--customer</option><oper class="+ topic/ph pr-d/oper " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="oper:2;27:38">=</oper><var class="+ topic/ph pr-d/var " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="var:6;27:51">name</var>
					[<option class="+ topic/keyword pr-d/option " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="option:8;28:15">--ticket</option><oper class="+ topic/ph pr-d/oper " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="oper:3;28:38">=</oper><var class="+ topic/ph pr-d/var " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="var:7;28:51">ticketNumber</var> ] ] </synph>
		</section>

		<section class="- topic/section " xtrf="/Users/Ruth/forks/docs-ng/dita/src/ag3/Features/cbcollect-info/topics/cluster-wide-info-cli-tool.dita" xtrc="section:2;31:12">
